WITH pivot_data AS (
  SELECT link, date, customer, clinic, business_line, protocol, modality, gender,
         step_1, step_2, step_3, step_4, step_5, visit_id, total_quick_start_visits
  FROM your_table
)

SELECT link, date, customer, clinic, business_line, protocol, modality, gender,
       visit_id, total_quick_start_visits, 
       steps, step_details
FROM pivot_data
CROSS JOIN UNNEST(ARRAY[step_1, step_2, step_3, step_4, step_5]) as t(step_details)
WITH ORDINALITY AS steps
}
